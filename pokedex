class Pokemon {
  var num = 0
  var name = ""
  var type = [String]()
  var ability = [String]()

    init(num: Int, name: String, type: [String], ability: [String]) {
      self.num = num
      self.name = name
      self.type = type
      self.ability = ability
    }
    func displayInfo() {
      print("Yours pokemon name is \(name), it's number is \(num), he's power type is \(type) and ability is \(ability)")
    }
}
class GigantamaxPokemon: Pokemon {
  var location = ""
    init(num: Int, name: String, type: [String], ability: [String], location: String){
      self.location = location
    super.init(num: num, name: name, type: type, ability: ability)
    }
    override func displayInfo() {
      print("Yours pokemon name is \(name), it's number is \(num), he's power type is \(type) and ability is \(ability) and his location is \(location)")
}
}

